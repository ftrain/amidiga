{
  "name": "lua",
  "version": "5.4.6",
  "description": "Lua 5.4.6 scripting language for embedded systems",
  "keywords": "lua, scripting, embedded",
  "repository": {
    "type": "git",
    "url": "https://github.com/lua/lua.git"
  },
  "homepage": "http://www.lua.org/",
  "license": "MIT",
  "frameworks": "arduino",
  "platforms": "teensy",
  "build": {
    "flags": [
      "-DLUA_32BITS",
      "-DLUA_USE_C89",
      "-DLUA_COMPAT_5_3",
      "-Wno-maybe-uninitialized"
    ],
    "srcFilter": [
      "+<*.c>",
      "-<lua.c>",
      "-<luac.c>",
      "-<onelua.c>"
    ],
    "includeDir": "."
  }
}
